<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="org.zinemanager.gui.controller.InfoController">
   <children>
      <HBox minHeight="-Infinity" minWidth="-Infinity">
         <children>
            <Label fx:id="lTitle" minHeight="-Infinity" minWidth="-Infinity" text="Label">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <ScrollPane minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
         <content>
            <TextArea fx:id="taMessage" editable="false" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="400.0" />
         </content>
      </ScrollPane>
      <Button fx:id="btBack" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="150.0" text="OK" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
