<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="org.zinemanager.gui.controller.zineinventory.singleeditor.SingleCategoryEditorController">
   <children>
      <Label fx:id="lTitle" minHeight="-Infinity" minWidth="-Infinity" text="Kategorie hinzufügen">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <HBox minHeight="-Infinity" minWidth="-Infinity" spacing="10.0">
         <children>
            <Label text="Kategorie:" />
            <TextField fx:id="tfName" />
         </children>
      </HBox>
      <VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0">
         <children>
            <Button fx:id="btProgress" mnemonicParsing="false" onAction="#progress" prefHeight="30.0" prefWidth="150.0" text="Hinzufügen" />
            <Button fx:id="btBack" mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="150.0" text="Abbrechen" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
